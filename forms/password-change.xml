<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.8.5">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Touristic - password change]]></title>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Промени паролата]]></text>
		</button>
		<fields>
			<field id="1" type="text" size="medium">
				<label><![CDATA[Текуща парола]]></label>
				<enablePasswordInput><![CDATA[1]]></enablePasswordInput>
			</field>
			<field id="2" type="text" size="medium">
				<label><![CDATA[Нова парола]]></label>
				<enablePasswordInput><![CDATA[1]]></enablePasswordInput>
			</field>
			<field id="3" type="text" size="medium">
				<label><![CDATA[Повтори паролата]]></label>
				<enablePasswordInput><![CDATA[1]]></enablePasswordInput>
			</field>
		</fields>
		<notifications>
			<notification id="54495db3723b3">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Admin Notification]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
			</notification>
		</notifications>
		<confirmations>
			<confirmation id="54495db3785d3" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
	</form>
</forms>